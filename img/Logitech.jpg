	<script type="text/javascript">
		function readCookie(name) {
			var start = document.cookie.indexOf(name + "=");
			var len = start + name.length + 1;
			if ((!start) && (name != document.cookie.substring(0, name.length))) {
				return null;
		  	}
			if (start == -1) return null;
		  	var end = document.cookie.indexOf(';', len);
		  	if (end == -1) end = document.cookie.length;
		  	return unescape(document.cookie.substring(len, end));
		}
		function getParameterByName(name, url) {
		    name = name.replace(/[\[\]]/g, "\\$&");
		    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
		        results = regex.exec(url);
		    if (!results) return null;
		    if (!results[2]) return '';
		    return decodeURIComponent(results[2].replace(/\+/g, " "));
		}
		var chkCookie = readCookie('bannerParams');
		//console.log('bannerParams defined ?',chkCookie);
		if(chkCookie == null){
			//console.log('chkCookie not defined');
			var currentLocation = window.location.href;
			var bnrParam1,bnrParam2;
			bnrParam1 = getParameterByName('WT.ac',currentLocation);
			if(bnrParam1 == null){
				bnrParam2 = getParameterByName('wt.ac',currentLocation);
			}
			var bnrNumber='';
			if(bnrParam1 != null){ bnrNumber = bnrParam1; }
			else if(bnrParam2 != null){ bnrNumber = bnrParam2; }
			if(bnrNumber != null){
				arrParam=bnrNumber.split('-');
				if(arrParam[0].search('HPB') == 0){
					chkCookie = arrParam[0].slice(3);
				}
			}
		}
		//console.log('chkCookie',chkCookie);
		if (chkCookie != null && chkCookie > 0){
			var pdtName,pdtFinalName,intFindHyphen=0;
			if('product_name' in utag_data){
				pdtName = utag_data['product_name'][0];
				intFindHyphen = pdtName.indexOf('-');
				if (intFindHyphen > 0){
					pdtFinalName = pdtName.slice(0,intFindHyphen-1);
				}
				else if(intFindHyphen == -1){
					pdtFinalName = pdtName;
				}
				//console.log('pdtFinalName',pdtFinalName);
			}
			else if ('page_name' in utag_data){
				arrPageName = utag_data['page_name'].split(':');
				if(arrPageName.length > 1){
					pdtFinalName = arrPageName[1];
				}
			}
			//eraseCookieFromAllPaths('bannerParams');
			utag_data['banner']=chkCookie + ':' + pdtFinalName;
			//console.log('readCookie',utag_data['banner']);
		}
	</script>
