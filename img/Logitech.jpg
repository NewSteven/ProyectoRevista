	jQuery.validator.addMethod("betterEmail", function(value, element) {
      // allow any non-whitespace characters as the host part
      return this.optional( element ) || /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/.test( value );
